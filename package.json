{
  "name": "manypapers",
  "version": "0.0.1",
  "description": "Host for papers",
  "scripts": {
    "watch": "cd client && gulp watch & cd client && webpack -w",
    "up": "cd server && NODE_ENV=development nodemon server.js -w . -w ../dist --ext js,njk",
    "db": "cd db; docker-compose -p paper-db up --build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./server ./client",
    "spawn": "gnome-terminal -- yarn db && gnome-terminal -- yarn watch && yarn up"
  },
